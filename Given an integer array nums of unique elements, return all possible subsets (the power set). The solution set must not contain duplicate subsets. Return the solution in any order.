def subsets(nums):
    result = []

    def backtrack(start, path):
        # store current subset
        result.append(path[:])

        for i in range(start, len(nums)):
            # choose element
            path.append(nums[i])

            # explore further
            backtrack(i + 1, path)

            # undo choice
            path.pop()

    backtrack(0, [])
    return result


# Examples
print(subsets([1,2,3]))
print(subsets([0]))
