def factorial(n):
    result = [1]   # stores digits in reverse order
    
    for num in range(2, n + 1):
        carry = 0
        
        for i in range(len(result)):
            product = result[i] * num + carry
            result[i] = product % 10
            carry = product // 10
        
        # handle remaining carry
        while carry:
            result.append(carry % 10)
            carry //= 10
    
    # reverse to get actual order
    result.reverse()
    return result
