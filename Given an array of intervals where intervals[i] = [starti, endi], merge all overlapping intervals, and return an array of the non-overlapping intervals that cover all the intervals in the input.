class Solution:
    def merge(self, intervals):
        if not intervals:
            return []

        # Step 1: Sort intervals based on start time
        intervals.sort(key=lambda x: x[0])

        merged = [intervals[0]]

        # Step 2: Traverse intervals
        for current in intervals[1:]:
            last = merged[-1]

            # If overlapping
            if current[0] <= last[1]:
                last[1] = max(last[1], current[1])
            else:
                merged.append(current)

        return merged
